<script>
    class tumbuhan1 {
        // atribut = kata sifat
        constructor() {
            this.nama = 'Mangga'
            this.jenis = 'Buah'
            this.warna = 'Kuning'
        }

        // fungsi = kata kerja
        tumbuh(tumbuh) {
            console.log("Tumbuhan ini dapat tumbuh")
        }
        
        berbuah(buah) {
            console.log("Tumbuhan ini menghasilkan buah " + buah)
        }

    }
    buahbuah = new tumbuhan1()
    console.log(tumbuhan1)


    class tumbuhan2 {
        // atribut = kata sifat
        constructor() {
            this.nama = 'Wortel'
            this.jenis = 'Sayur'
            this.warna = 'Orange'
        }

        // fungsi = kata kerja
        tumbuh(tumbuh) {
            console.log("Tumbuhan ini dapat tumbuh")
        }
        
        sayuran(sayur) {
            console.log("Tumbuhan ini menghasilkan sayur " + sayur)
        }

    }
    sayuran = new tumbuhan2()
    console.log(tumbuhan2)


    class tumbuhan3 {
        // atribut = kata sifat
        constructor() {
            this.nama = 'Mawar'
            this.jenis = 'Bunga'
            this.warna = 'Merah'
        }

        // fungsi = kata kerja
        tumbuh(tumbuh) {
            console.log("Tumbuhan ini dapat tumbuh")
        }
        
        berbunga(bunga) {
            console.log("Tumbuhan ini menghasilkan bunga " + bunga)
        }

    }
    bunga = new tumbuhan3()
    console.log(tumbuhan3)
</script>

